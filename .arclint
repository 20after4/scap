{
  "linters": {
    "pep8": {
      "type": "pep8",
      "include": "(\\.py$|^bin/.+$)",
      "exclude": "(^bin/refreshCdbJsonFiles$)"
    },
    "pyflakes": {
      "type": "pyflakes",
      "include": "(\\.py$|^bin/.+$)",
      "exclude": "(^bin/refreshCdbJsonFiles$)"
    },
    "php": {
      "type": "php",
      "include": "(\\.php$|^bin/refreshCdbJsonFiles$)"
    }
  }
}
